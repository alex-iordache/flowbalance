{
	"appId": "com.flowapp.app",
	"appName": "Flow",
	"bundledWebRuntime": false,
	"npmClient": "npm",
	"webDir": "out",
	"server": {
		"url": "https://www.flowbalance.app",
		"cleartext": false,
		"androidScheme": "https",
		"allowNavigation": [
			"www.flowbalance.app",
			"flowbalance.app",
			"flowbalance.vercel.app",
			"*.vercel.app",
			"clerk.com",
			"*.clerk.com",
			"accounts.clerk.dev",
			"*.clerk.accounts.dev",
			"clerk.accounts.dev",
			"clerk.shared.lcl.dev",
			"*.lcl.dev"
		]
	},
	"plugins": {
		"SplashScreen": {
			"launchShowDuration": 0
		},
		"CapacitorCookies": {
			"enabled": true
		},
		"CapacitorHttp": {
			"enabled": false
		}
	},
	"cordova": {},
	"packageClassList": [
		"AppPlugin",
		"AppLauncherPlugin",
		"CAPBrowserPlugin",
		"HapticsPlugin",
		"KeyboardPlugin",
		"PreferencesPlugin",
		"StatusBarPlugin"
	]
}
