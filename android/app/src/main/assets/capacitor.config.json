{
	"appId": "com.flowapp.app",
	"appName": "Flow",
	"bundledWebRuntime": false,
	"npmClient": "npm",
	"webDir": "out",
	"loggingBehavior": "debug",
	"zoomEnabled": false,
	"initialFocus": false,
	"server": {
		"url": "https://www.flowbalance.app",
		"cleartext": false,
		"androidScheme": "https",
		"errorPath": "offline.html",
		"allowNavigation": [
			"www.flowbalance.app",
			"flowbalance.app",
			"flowbalance.vercel.app",
			"*.vercel.app",
			"clerk.flowbalance.app",
			"accounts.flowbalance.app",
			"clkmail.flowbalance.app",
			"clerk.com",
			"*.clerk.com",
			"accounts.clerk.dev",
			"popular-gibbon-62.clerk.accounts.dev",
			"*.clerk.accounts.dev",
			"popular-gibbon-62.accounts.dev",
			"*.accounts.dev",
			"clerk.accounts.dev",
			"clerk.shared.lcl.dev",
			"*.lcl.dev"
		]
	},
	"plugins": {
		"SplashScreen": {
			"launchShowDuration": 0
		},
		"CapacitorCookies": {
			"enabled": true
		},
		"CapacitorHttp": {
			"enabled": false
		}
	},
	"cordova": {}
}
