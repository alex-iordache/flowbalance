{
  "appId": "com.flowapp.app",
  "appName": "Flow",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "out",
  "server": {
    "url": "https://flowbalance-jdk.vercel.app",
    "cleartext": false,
    "androidScheme": "https",
    "allowNavigation": [
      "flowbalance-jdk.vercel.app",
      "*.vercel.app",
      "clerk.com",
      "*.clerk.com",
      "accounts.clerk.dev",
      "*.clerk.accounts.dev",
      "clerk.accounts.dev",
      "clerk.shared.lcl.dev",
      "*.lcl.dev"
    ]
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0
    },
    "CapacitorCookies": {
      "enabled": true
    },
    "CapacitorHttp": {
      "enabled": false
    }
  },
  "cordova": {}
}
