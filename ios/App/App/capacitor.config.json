{
	"appId": "com.flowapp.app",
	"appName": "Flow",
	"bundledWebRuntime": false,
	"npmClient": "npm",
	"webDir": "out",
	"server": {
		"url": "https://flowbalance-jdk.vercel.app",
		"cleartext": false,
		"androidScheme": "https",
		"allowNavigation": [
			"flowbalance-jdk.vercel.app",
			"*.vercel.app",
			"accounts.clerk.dev",
			"*.clerk.accounts.dev",
			"clerk.shared.lcl.dev"
		]
	},
	"plugins": {
		"SplashScreen": {
			"launchShowDuration": 0
		},
		"CapacitorCookies": {
			"enabled": true
		}
	},
	"cordova": {},
	"packageClassList": [
		"PreferencesPlugin",
		"StatusBarPlugin",
		"AppPlugin",
		"BrowserPlugin"
	]
}
